<%- include('partials/_header') %> 
<div class="container mt-5">
    <div class="row">
        <div class="col-md-5">
            <div class="card">
                <div class="card-body">
                    <form action="/update/<%= data.id %>" method="post">
                        <div class="form-group m-2">
                            <input type="text" name="name" value="<%= data.name %>" class="form-control">
                        </div>
                        <div class="form-group m-2">
                            <input type="text" name="correo" value="<%= data.correo %>" class="form-control">
                        </div>
                        <div class="form-group m-2">
                            <input type="text" name="edad" value="<%= data.edad %>" class="form-control">
                        </div>
                        <button type="submit" class="btn btn-primary">
                            EDITAR USUARIO
                        </button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
<%- include('partials/_footer') %> 